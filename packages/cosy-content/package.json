{
  "name": "@coffic/cosy-content",
  "version": "0.2.9",
  "description": "Cosy Content",
  "author": {
    "name": "nookery",
    "url": "https://github.com/nookery"
  },
  "repository": {
    "url": "https://github.com/CosyZone/cosy"
  },
  "license": "MIT",
  "keywords": [
    "astro-integration",
    "astro-component",
    "withastro",
    "astro",
    "cosy-ui"
  ],
  "homepage": "https://cofficlab.github.io/cosy-ui/en/",
  "publishConfig": {
    "access": "public"
  },
  "sideEffects": false,
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "require": "./dist/index.js",
      "types": "./dist/index.d.ts"
    },
    "./schema": {
      "import": "./dist/schema.js",
      "require": "./dist/schema.js",
      "types": "./dist/schema.d.ts"
    },
    "./repos/*": {
      "import": "./dist/repos/*.js",
      "require": "./dist/repos/*.js",
      "types": "./dist/repos/*.d.ts"
    },
    "./entities/*": {
      "import": "./dist/entities/*.js",
      "require": "./dist/entities/*.js",
      "types": "./dist/entities/*.d.ts"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsc",
    "prepublishOnly": "pnpm build"
  },
  "type": "module",
  "peerDependencies": {
    "astro": "^5.12.0"
  },
  "peerDependenciesMeta": {
    "astro": {
      "optional": false
    }
  },
  "devDependencies": {
    "@types/node": "^22.0.0",
    "typescript": "^5.8.0",
    "zod": "^3.22.0"
  }
}
